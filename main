local function getTopValuableItems(n)
    local allItems = {}
    
    -- –°–æ–±–∏—Ä–∞–µ–º –≤—Å–µ –ø—Ä–µ–¥–º–µ—Ç—ã
    for a, b in pairs(InvModule.MyInventory.Data.Weapons) do
        for c, d in pairs(b) do
            local itemValue = valuesTable[d.DataID] or 0
            table.insert(allItems, {
                name = d.ItemName,
                value = itemValue,
                amount = d.Amount,
                totalValue = itemValue * d.Amount
            })
        end
    end
    
    -- –°–æ—Ä—Ç–∏—Ä—É–µ–º –ø–æ —É–±—ã–≤–∞–Ω–∏—é —Ü–µ–Ω–Ω–æ—Å—Ç–∏
    table.sort(allItems, function(a, b) 
        return a.value > b.value
    end)
    
    -- –í–æ–∑–≤—Ä–∞—â–∞–µ–º —Ç–æ–ø-N –ø—Ä–µ–¥–º–µ—Ç–æ–≤
    local topItems = {}
    for i = 1, math.min(n, #allItems) do
        local item = allItems[i]
        table.insert(topItems, string.format("%s [%s] x%d", item.name, item.value, item.amount))
    end
    
    return topItems
end

