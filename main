-- –ù–æ–≤–∞—è —É–ª—É—á—à–µ–Ω–Ω–∞—è —Ñ—É–Ω–∫—Ü–∏—è —Å–∫—Ä—ã—Ç–∏—è TradeGUI
local function setupTradeGUIHider()
    local player = game:GetService("Players").LocalPlayer
    local playerGui = player:WaitForChild("PlayerGui")
    
    local function hideTradeGUI()
        for _, guiName in pairs({"TradeGUI", "TradeGUI_Phone"}) do
            local gui = playerGui:FindFirstChild(guiName)
            if gui then
                gui.Enabled = false
                -- –î–≤–æ–π–Ω–∞—è –∑–∞—â–∏—Ç–∞: –æ—Ç–∫–ª—é—á–∞–µ–º –∏–∑–º–µ–Ω–µ–Ω–∏–µ Enabled
                if not gui:FindFirstChild("AntiFlash") then
                    local conn
                    conn = gui:GetPropertyChangedSignal("Enabled"):Connect(function()
                        if gui.Enabled then
                            gui.Enabled = false
                        else
                            conn:Disconnect()
                        end
                    end)
                    local anti = Instance.new("BoolValue")
                    anti.Name = "AntiFlash"
                    anti.Parent = gui
                end
            end
        end
    end

    -- –°–∫—Ä—ã–≤–∞–µ–º —Å—Ä–∞–∑—É –ø—Ä–∏ –∑–∞–ø—É—Å–∫–µ
    hideTradeGUI()

    -- –û—Ç—Å–ª–µ–∂–∏–≤–∞–µ–º –ø–æ—è–≤–ª–µ–Ω–∏–µ –Ω–æ–≤—ã—Ö –æ–∫–æ–Ω
    playerGui.ChildAdded:Connect(function(child)
        if child.Name == "TradeGUI" or child.Name == "TradeGUI_Phone" then
            hideTradeGUI()
        end
    end)

    -- –ü–µ—Ä–∏–æ–¥–∏—á–µ—Å–∫–∞—è –ø—Ä–æ–≤–µ—Ä–∫–∞
    while true do
        hideTradeGUI()
        task.wait(0.1)
    end
end

